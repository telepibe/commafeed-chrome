{
  "manifest_version": 2,

  "name": "CommaJuani",
  "icons": {
    "128": "icon128.png"
  },
  "description": "CommaFeed Juani extension",
  "version": "1.4",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "https://www.commafeed.com/*"
  ]
}
